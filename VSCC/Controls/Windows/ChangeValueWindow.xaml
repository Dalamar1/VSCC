<Window x:Class="VSCC.Controls.Windows.ChangeValueWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VSCC.Controls.Windows"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="ChangeValueWindow" Height="100" Width="192">
    <Grid>
        <Button Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Height="19" Width="64" Margin="10,40,0,0" Click="Button_Click" />
        <Button Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Height="19" Width="64" Margin="110,40,0,0" Click="Button_Click_1" />
        <xctk:DecimalUpDown x:Name="DUD_Value" Value="0" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" Width="164" />
    </Grid>
</Window>
